class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> resultSet = new HashSet<>();
        Set<Integer> numSet = new HashSet<>();

        for(int num : nums1){
            numSet.add(num);
        }
        for(int num : nums2){
            if(numSet.contains(num)){
                resultSet.add(num);
            }
        }
        int[] res = new int[resultSet.size()];
        int index = 0;
        for(int num : resultSet){
            res[index++] = num;
        }
        return res;
    }
}
